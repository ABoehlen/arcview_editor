(SEd.258
	Name:	"Editor.AutoCompleteUpdate"
	Creator:	"adrian"
	CreationDate:	"Fre 17 Dez 2021 08:30:53 CET"
	GUIName:	"Script"
	Win:	259
	CSMgr:	260
	DocExts:	261
	Source:	"' update script for the built-in function View.AutoCompleteTool\n' 20.12.2021, A. Boehlen\n'###############################\n\ntheView = av.GetActiveDoc\ntheTheme = theView.GetEditableTheme\n\nif (theTheme = nil) then\n  SELF.SetEnabled(False) \nelse\n  theType = theTheme.GetFTab.FindField(\"Shape\").GetType\n  SELF.SetEnabled(theType = #FIELD_SHAPEPOLY)\nend  \n"
)

