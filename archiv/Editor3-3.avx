(Script.160
	Name:	"Editor.AutoComplete"
	SourceCode:	"' uses the built-in function View.AutoCompleteTool\n\n' ################################\n' REV: 11.01.2026, A. Boehlen\n' - Check theme\n' - Set timestamp for new features\n' ################################\n\ntheView = av.GetActiveDoc\ntheTheme = theView.GetEditableTheme\n\nif (av.Run(\"Editor.SubCheckTheme\", {theTheme, \"polygon\", \"false\", \"0\"}) = nil) then\n  return nil\nend\n\ntheFTab = theTheme.GetFTab\n\n'# create string field DateOfCr if it doesn't exist\nif (theFTab.FindField(\"DateOfCr\") = nil) then\n  av.Run(\"Editor.
SubCreateStrField\", {theFTab, \"DateOfCr\", \"16\"})\nend\n\nl = theView.ReturnUserPolyLine\nif (l.IsNull) then\n  return nil\nelse\n  theFtab.BeginTransaction\n  theShpFld = theFTab.FindField(\"Shape\")\n  theDate = theTheme.GetFTab.FindField(\"DateOfCr\")\n\n  theTheme.AutoComplete(l)\n\n  '# set timestamp as string for the selected features\n  theBitmap = theFTab.getSelection \n\n  for each rec in theBitmap\n    theVal = theFTab.ReturnValue (theShpFld, rec)\n    if ((theVal.asString) = \"Polyline: Null\") then\n      theBitmap.set(r
ec)\n    else\n      timestampStr = av.Run(\"Editor.SubCreateDate\", {0})\n      theFTab.SetValue(theDate,rec,timestampStr)\n\n      theBitmap.clear(rec)\n    end\n  end\n\n  theFtab.EndTransaction  \n  av.GetProject.SetModified(true)\nend\n\n'# refresh view\nav.Run(\"Editor.SubRefreshView\", {theView})\n"
)

(Script.161
	Name:	"Editor.AutoCompleteUpdate"
	SourceCode:	"' update script for the built-in function View.AutoCompleteTool\n' 20.12.2021, A. Boehlen\n'###############################\n\ntheView = av.GetActiveDoc\ntheTheme = theView.GetEditableTheme\n\nif (theTheme = nil) then\n  SELF.SetEnabled(False) \nelse\n  theType = theTheme.GetFTab.FindField(\"Shape\").GetType\n  SELF.SetEnabled(theType = #FIELD_SHAPEPOLY)\nend  \n"
)

(Script.162
	Name:	"Editor.CircleTool"
	SourceCode:	"' uses the built-in script View.CircleTool\n\n' ################################\n' REV: 10.01.2026, A. Boehlen\n' - Check theme\n' - Set timestamp for new features\n' ################################\n\ntheView = av.GetActiveDoc\ntheTheme = theView.GetEditableTheme\n\nif (av.Run(\"Editor.SubCheckTheme\", {theTheme, \"polygon\", \"false\", \"0\"}) = nil) then\n  return nil\nend\n\ntheFTab = theTheme.GetFTab\n\n'# create string field DateOfCr if it doesn't exist\nif (theFTab.FindField(\"DateOfCr\") = nil) then\n  av.Run(\"Editor.SubCreat
eStrField\", {theFTab, \"DateOfCr\", \"16\"})\nend\n\nc = theView.ReturnUserCircle\nif (c.IsNull) then\n  return nil\nelse\n  theTheme.GetFTab.BeginTransaction\n  p = c.AsPolygon\n  thePrj = theView.GetProjection\n  if (thePrj.IsNull.Not) then\n    p = p.ReturnUnprojected(thePrj)\n  end\n  theField = theFTab.FindField(\"Shape\")\n  theDate = theTheme.GetFTab.FindField(\"DateOfCr\")\n  rec = theFTab.AddRecord\n  theFTab.SetValue(theField, rec, p)\n  theFTab.GetSelection.ClearAll\n  theFTab.GetSelection.Set(rec)\n  theFTab.UpdateSelecti
on\n  \n  '# set timestamp as string\n  timestampStr = av.Run(\"Editor.SubCreateDate\", {0})\n  theFTab.SetValue(theDate,rec,timestampStr)\n\n  theFTab.EndTransaction\n  av.GetProject.SetModified(true)\nend\n"
)

